{
    "name": "<%= projectNamespacedName %>",
    "version": "<%= projectVersion %>",
    "description": "<%= projectDescription %>",
    "keywords": <%- JSON.stringify(projectKeywords) %>,
    "author": "<%= authorName %> <<%= authorEmail %>>",
    "dependencies": {
        "@blueprintjs/core": "^3.15.1",
        "@vamship/arg-utils": "^2.3.0",
        "debug": "^4.1.1",
        "mobx": "^5.9.4",
        "mobx-react": "^5.4.3",
        "node-sass": "^4.12.0",
        "prettier": "^1.17.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "3.0.0",
        "tachyons": "^4.11.1"
    },
    "repository": {
        "type": "git",
        "url": "https://<%= gitUrl %>.git"
    },
    "bugs": {
        "url": "https://<%= gitUrl %>/issues"
    },
    "homepage": "https://<%= gitUrl %>#readme",
    "scripts": {
        "clean": "rm -rf build",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint src/**/*.js",
        "format": "prettier --write \"{{src,test}/**/*.js,README.md}\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"],
    "license": "MIT"
}
