{
    "name": "<%= projectNamespacedName %>",
    "version": "<%= projectVersion %>",
    "description": "<%= projectDescription %>",
    "main": "src/index.js",
    "scripts": {
        "all": "gulp --series format lint test-unit clean",
        "test": "gulp test-unit",
        "start": "node src/index"
    },
    "files": [ "package.json", "LICENSE", "README.md", "src/**/*" ],
    "author": "<%= authorName %> <<%= authorEmail %>>",
    "keywords": <%- JSON.stringify(projectKeywords) %>,
    "license": "MIT",
    "devDependencies": {
        "@vamship/build-utils": "0.2.0",
        "@vamship/test-utils": "^2.4.9",
        "ansi-colors": "^4.1.1",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-http": "^4.3.0",
        "docdash": "^1.1.1",
        "gulp": "^4.0.2",
        "gulp-jsdoc3": "^2.0.0",
        "mocha": "^7.0.0",
        "nyc": "^15.0.0",
        "rewire": "^4.0.1",
        "sinon": "^8.1.0",
        "sinon-chai": "^3.4.0",
        "sinon-express-mock": "^2.2.1"
    },
    "dependencies": {
        "@vamship/arg-utils": "^2.4.6",
        "@vamship/config": "^1.4.6",
        "@vamship/error-types": "^1.7.8",
        "@vamship/expressjs-routes": "^1.7.2",
        "@vamship/logger": "^4.2.6",
        "ajv": "^6.11.0",
        "bluebird": "^3.7.2",
        "express": "^4.17.1"
    },
    "repository": {
        "type": "git",
        "url": "https://<%= gitUrl %>.git"
    },
    "bugs": {
        "url": "https://<%= gitUrl %>/issues"
    },
    "homepage": "https://<%= gitUrl %>#readme",
    "buildMetadata": {
        "projectType": "api",
<% if(dockerRequired) {-%>
        "language": "js",
        "docker": {
<%   if(dockerFullRepo) {-%>
            "repo": "<%= dockerFullRepo %>"
<%   } else if(dockerCustomRegistry) {-%>
            "registry": "<%= dockerCustomRegistry %>"
<%   }-%>
        }
<% } else {-%>
        "language": "js"
<% }-%>
    }
}
