{
  "name": "@vamship/generator-javascript",
  "version": "1.2.0",
  "description": "Yeoman generator for javascript projects",
  "homepage": "",
  "author": {
    "name": "Vamshi K Ponnapalli",
    "email": "vamshi.ponnapalli@gmail.com",
    "url": ""
  },
  "files": [
    "generators",
    "utils"
  ],
  "main": "generators/index.js",
  "keywords": [
    "generators",
    "javascript",
    "utils",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.9.1",
    "yeoman-assert": "^3.1.1",
    "eslint": "^5.12.0",
    "prettier": "^1.15.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-config-xo": "^0.26.0",
    "jest": "^23.6.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "chalk": "^2.4.2",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "vamship/generator-javascript",
  "license": "MIT"
}
